{
    "name": "fsyd88/yii2-admin",
    "description": "RBAC Auth manager And Adminlte for Yii2 ",
    "keywords": ["yii", "admin", "auth", "rbac","adminlte"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/fsyd88/yii2-admin/issues",
        "source": "https://github.com/fsyd88/yii2-admin"
    },
    "authors": [
        {
            "name": "fsyd",
            "email": "fsyd88@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0.7",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "almasaeed2010/adminlte": "^2.4.0",
        "rmrevin/yii2-fontawesome": "~2.9"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "Used when using layout 'main', 'main-index' or 'main_single_page'"
    },
    "autoload": {
        "psr-4": {
            "fsyd\\admin\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
